{
    "name": "destructure-backend",
    "version": "1.0.6",
    "description": "Destructure app backend",
    "main": "src/index.ts",
    "scripts": {
        "type-check": "tsc --noEmit",
        "build": "tsc && tsc-alias && tsx ./src/utils/build.util.ts",
        "start": "node dist/index.js",
        "dev": "tsx watch src/run.ts --clear-screen=false",
        "lint": "eslint ./src/",
        "format": "prettier ./src --write",
        "format:check": "prettier ./src --check",
        "spell": "cspell lint .",
        "lefthook": "tsx ./.git_hooks/commitMessage.ts",
        "check": "pnpm type-check && pnpm lint && pnpm format:check && pnpm spell"
    },
    "keywords": [
        "NodeJS",
        "Express",
        "MongoDB"
    ],
    "author": "Shakti Ranjan Debata",
    "license": "ISC",
    "dependencies": {
        "cookie-parser": "^1.4.7",
        "cors": "^2.8.5",
        "dotenv": "^17.2.3",
        "express": "^5.2.1",
        "google-auth-library": "^10.5.0",
        "jsonwebtoken": "^9.0.3",
        "lorin": "2.0.3",
        "mongoose": "^9.1.1",
        "multer": "2.0.2",
        "plaiceholder": "^3.0.0",
        "sharp": "^0.34.5",
        "swagger-ui-express": "^5.0.1",
        "yaml": "^2.8.2",
        "zod": "^4.3.4"
    },
    "devDependencies": {
        "@eslint/js": "^9.39.2",
        "@types/cookie-parser": "^1.4.10",
        "@types/cors": "^2.8.19",
        "@types/express": "^5.0.6",
        "@types/jsonwebtoken": "^9.0.10",
        "@types/multer": "^2.0.0",
        "@types/swagger-ui-express": "^4.1.8",
        "@typescript-eslint/eslint-plugin": "^8.51.0",
        "@typescript-eslint/parser": "^8.51.0",
        "cspell": "^9.4.0",
        "eslint": "^9.39.2",
        "globals": "^17.0.0",
        "husky": "^9.1.7",
        "prettier": "^3.7.4",
        "tsc-alias": "^1.8.16",
        "tsx": "^4.21.0",
        "typescript": "^5.9.3",
        "typescript-eslint": "^8.51.0"
    },
    "packageManager": "pnpm@10.13.1"
}
