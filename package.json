{
    "name": "destructure-backend",
    "version": "1.0.6",
    "description": "Destructure app backend",
    "main": "src/index.ts",
    "scripts": {
        "type-check": "tsc --noEmit",
        "build": "tsc && tsx ./src/utils/build.util.ts",
        "start": "node dist/index.js",
        "dev": "pnpm type-check & tsx watch --clear-screen=false src/index.ts",
        "lint": "eslint ./src/",
        "format": "prettier ./src --write",
        "format.check": "prettier ./src --check",
        "spell": "cspell lint .",
        "lefthook": "tsx ./.git_hooks/commitMessage.ts"
    },
    "keywords": [
        "NodeJS",
        "Express",
        "MongoDB"
    ],
    "author": "Shakti Ranjan Debata",
    "license": "ISC",
    "dependencies": {
        "cookie-parser": "^1.4.7",
        "cors": "^2.8.5",
        "dotenv": "^17.2.0",
        "express": "^5.1.0",
        "google-auth-library": "^10.1.0",
        "jsonwebtoken": "^9.0.2",
        "lorin": "2.0.3",
        "mongoose": "^8.16.4",
        "multer": "2.0.2",
        "plaiceholder": "^3.0.0",
        "sharp": "^0.34.3",
        "swagger-ui-express": "^5.0.1",
        "yaml": "^2.8.0",
        "zod": "^4.0.5"
    },
    "devDependencies": {
        "@eslint/js": "^9.31.0",
        "@types/cookie-parser": "^1.4.9",
        "@types/cors": "^2.8.19",
        "@types/express": "^5.0.3",
        "@types/jsonwebtoken": "^9.0.10",
        "@types/multer": "^2.0.0",
        "@types/swagger-ui-express": "^4.1.8",
        "@typescript-eslint/eslint-plugin": "^8.37.0",
        "@typescript-eslint/parser": "^8.37.0",
        "cspell": "^9.1.5",
        "eslint": "^9.31.0",
        "globals": "^16.3.0",
        "lefthook": "^1.12.2",
        "prettier": "^3.6.2",
        "tsx": "^4.20.3",
        "typescript": "^5.8.3",
        "typescript-eslint": "^8.37.0"
    },
    "packageManager": "pnpm@10.13.1"
}
