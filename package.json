        "jsonwebtoken": "^9.0.2",
        "lorin": "2.0.3",
        "mongoose": "^8.14.1",
        "multer": "1.4.5-lts.2",
        "plaiceholder": "^3.0.0",
        "sharp": "^0.34.1",
        "zod": "^3.24.3"
    },
    "devDependencies": {
        "@eslint/js": "^9.25.1",
        "@types/cookie-parser": "^1.4.8",
        "@types/cors": "^2.8.17",
        "@types/express": "^5.0.1",
        "@types/jsonwebtoken": "^9.0.9",
        "@types/multer": "^1.4.12",
        "@typescript-eslint/eslint-plugin": "^8.31.1",
        "@typescript-eslint/parser": "^8.31.1",
        "cspell": "^8.19.3",
        "eslint": "^9.25.1",
        "globals": "^16.0.0",
        "lefthook": "^1.11.12",
        "nodemon": "^3.1.10",
        "prettier": "^3.5.3",
        "ts-node": "^10.9.2",
        "typescript": "^5.8.3",
        "typescript-eslint": "^8.31.1"
